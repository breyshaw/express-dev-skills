<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express Dev Skills</title>
</head>

<body>
    <h1>Skills</h1>
    <% if(error) { %>
        <h2>
            <%= error %>
        </h2>
        <% } %>
            <ul>
                <% skills.forEach(function(skill) { %>
                    <li>
                        <%= skill.text %>
                            -
                            <%= skill.need_to_work_on ? 'Need to work on' : 'Im doing well on this skill' %>
                    </li>
                    <% }) %>
            </ul>
</body>

</html>